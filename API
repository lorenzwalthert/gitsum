init_gitsum(path = ".", over_write = FALSE)
parse_log_detailed(path = ".", update_dump = TRUE)
parse_log_detailed_full_run(path = ".", na_to_zero = TRUE, file_name = NULL, commit_range = NULL)
parse_log_simple(path = ".", file_name = NULL)
read_log(path = ".")
remove_gitsum(path = ".")
report_git(path = ".", output_file = NULL, output_format = "html_document", template = "gitsum::repo_summary_simple", input_file = NULL, directory = "gitsum", cached = TRUE, .libpath_index = 1)
set_changed_file_to_latest_name(log)
